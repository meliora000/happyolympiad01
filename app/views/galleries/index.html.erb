<div class="section">
  <div class="row">
    <div class="col s12 l12">
      <iframe width="100%" height="500" src="https://www.youtube.com/embed/fQ_zV9xlU34" frameborder="0" allowfullscreen></iframe>
    </div>
  </div>
  <div class="row">
    <% @galls.each do |gall| %>

        <div class="col s12 l6">
          <% if gall.avatar.present? %>
              <%= image_tag(gall.avatar, :class => "picture", style: 'width:100%') %>
          <% end %>
        </div>

    <% end %>
  </div>
</div>


